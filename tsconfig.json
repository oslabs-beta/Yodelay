{
  "exclude": ["node_modules", "**/*.spec.ts", "server"],
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "target": "es5",
    "lib": ["es5", "es6", "dom"],
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "downlevelIteration": true,
    "traceResolution": true
  }
}
